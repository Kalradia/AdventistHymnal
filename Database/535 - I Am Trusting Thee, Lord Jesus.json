{
  "Number": 535,
  "Title": "I Am Trusting Thee, Lord Jesus",
  "Author": null,
  "Copyright": "PublicDomain",
  "CcliNumber": null,
  "Stanzas": [
    {
      "Lines": [
        "I am trusting Thee, Lord Jesus,",
        "Trusting only Thee;",
        "Trusting Thee for full salvation,",
        "Great and free."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 1
      }
    },
    {
      "Lines": [
        "I am trusting Thee for pardon;",
        "At Thy feet I bow;",
        "For Thy grace and tender mercy,",
        "Trusting now."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 2
      }
    },
    {
      "Lines": [
        "I am trusting Thee to guide me;",
        "Thou alone shalt lead;",
        "Every day and hour supplying",
        "All my need."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 3
      }
    },
    {
      "Lines": [
        "I am trusting Thee, Lord Jesus;",
        "Never let me fall;",
        "I am trusting Thee forever,",
        "And for all."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 4
      }
    }
  ],
  "StanzaOrder": [
    "Verse 1",
    "Verse 2",
    "Verse 3",
    "Verse 4"
  ]
}