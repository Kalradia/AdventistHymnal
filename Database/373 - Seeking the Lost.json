{
  "Number": 373,
  "Title": "Seeking the Lost",
  "Author": null,
  "Copyright": "PublicDomain",
  "CcliNumber": null,
  "Stanzas": [
    {
      "Lines": [
        "Seeking the lost,",
        "yes, kindly entreating",
        "Wanderers on the mountain astray;",
        "“Come unto Me,”",
        "His message repeating,",
        "Words of the Master speaking today."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 1
      }
    },
    {
      "Lines": [
        "Going afar",
        "upon the mountain",
        "Bringing the wanderer back again,",
        "back again,",
        "Into the fold",
        "Of my Redeemer",
        "Jesus the Lamb for sinners slain,",
        "for sinners slain."
      ],
      "Reference": {
        "Type": "Chorus",
        "Number": 0
      }
    },
    {
      "Lines": [
        "Seeking the lost",
        "and pointing to Jesus",
        "Souls that are weak",
        "and hearts that are sore,",
        "Leading them forth",
        "in ways of salvation,",
        "Showing the path to life evermore."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 2
      }
    },
    {
      "Lines": [
        "Thus would I go",
        "on missions of mercy,",
        "Following Christ from day unto day,",
        "Cheering the faint",
        "and raising the fallen,",
        "Pointing the lost to Jesus, the Way."
      ],
      "Reference": {
        "Type": "Verse",
        "Number": 3
      }
    }
  ],
  "StanzaOrder": [
    "Verse 1",
    "Chorus",
    "Verse 2",
    "Chorus",
    "Verse 3",
    "Chorus"
  ]
}